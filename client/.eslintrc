{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": "airbnb",
  "plugins": [
    "react"
  ],
  "globals": {
    "__DEV__": true
  },
  "rules": {
        "no-unused-vars": "warn",
        "react/prop-types": "warn", // TODO change this
        "operator-linebreak": "off",
        "implicit-arrow-linebreak": "off"
  }
}
